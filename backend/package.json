{
  "name": "quickfold-backend",
  "version": "1.0.0",
  "description": "QuickFold Backend API",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:db": "node scripts/test-connection.js",
    "db:migrate": "node scripts/migrate.js",
    "db:seed": "node scripts/seed.js",
    "setup:env": "node scripts/setup-env.js",
    "setup:dev": "cp env.development .env && echo 'Development environment configured'",
    "setup:staging": "cp env.staging .env && echo 'Staging environment configured'",
    "setup:prod": "cp env.production .env && echo 'Production environment configured'",
    "verify:setup": "node scripts/verify-setup.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "sequelize": "^6.32.1",
    "mysql2": "^3.6.0",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.10.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.2"
  },
  "keywords": ["quickfold", "laundry", "api"],
  "author": "QuickFold Team",
  "license": "MIT"
}
